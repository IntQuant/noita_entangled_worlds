{
    "Lua.runtime.version": "LuaJIT",
    "Lua.diagnostics.disable": [
        "cast-local-type",
        "param-type-mismatch",
        "undefined-global",
        "need-check-nil"
    ],
    "Lua.diagnostics.globals": [
        "wait",
        "async"
    ],
    "C_Cpp.default.compilerPath": "/usr/bin/clang++",
    "spellright.language": [],
    "spellright.documentTypes": [
        "markdown",
        "latex"
    ],
    "rust-analyzer.linkedProjects": [
        "ewext/Cargo.toml",
        "noita-proxy/Cargo.toml",
    ]
}