{
    "Lua.runtime.version": "LuaJIT",
    "Lua.diagnostics.disable": [
        "cast-local-type",
        "param-type-mismatch",
        "undefined-global",
        "need-check-nil",
        "lowercase-global",
        "redundant-parameter",
        "missing-fields"
    ],
    "Lua.diagnostics.globals": [
        "wait",
        "async"
    ],
    "C_Cpp.default.compilerPath": "/usr/bin/clang++",
    "spellright.language": [],
    "spellright.documentTypes": [
        "markdown",
        "latex"
    ]
}